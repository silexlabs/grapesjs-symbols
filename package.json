{
  "type": "module",
  "name": "@silexlabs/grapesjs-symbols",
  "version": "1.0.20",
  "description": "Symbols for GrapesJS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/silexlabs/grapesjs-symbols.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "start": "grapesjs-cli serve",
    "build": "grapesjs-cli build --patch=false",
    "prepublishOnly": "$npm_execpath run build && $npm_execpath run test && $npm_execpath run lint",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "$npm_execpath run lint -- --fix",
    "test": "node --experimental-vm-modules `node_modules jest`/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules `node_modules jest`/jest/bin/jest.js --watch",
    "bump": "$npm_execpath version patch -m 'Bump v%s'"
  },
  "keywords": [
    "grapesjs",
    "plugin",
    "grapesjs-plugin"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-types": "^6.26.0",
    "eslint": "^8.43.0",
    "grapesjs": "^0.21.2",
    "grapesjs-cli": "^4.1.1",
    "jest": "^29.5.0",
    "node_modules-path": "^2.0.7",
    "ts-jest": "^29.1.0"
  },
  "license": "MIT",
  "dependencies": {
    "backbone": "^1.4.1",
    "jquery": "^3.7.0",
    "lit-html": "^2.7.4"
  }
}
